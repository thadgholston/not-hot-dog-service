[
  {
    "name": "api",
    "image": "python:3.7.2-stretch",
    "cpu": 100,
    "memory": 100,
    "portMappings": [
      {
        "hostPort": 443,
        "containerPort": 443,
        "protocol": "tcp"
      }
    ],
    "environment": [
      {
        "name": "VARNISH_NAMED_BACKEND",
        "value": "${backend_host}"
      },
      {
        "name": "ALLOWED_HOSTS",
        "value": "${allowed_hosts}"
      },
      {
        "name": "HEALTH_CHECK_URL",
        "value": "${health_check_url}"
      },
      {
        "name": "ADMIN_URL",
        "value": "${admin_url}"
      }
    ],
    "volumesFrom": [],
    "links": [],
    "mountPoints": [],
    "essential": true
  }
]
